" Settings
so ~/.vim/plugins.vim
so ~/.vim/settings/config-status-bar.vim
so ~/.vim/settings/config-nerdtree.vim
so ~/.vim/settings/config-syntax.vim
so ~/.vim/settings/config-snippets.vim
so ~/.vim/settings/config-maps.vim

" Setting base
set number
set mouse=a
set numberwidth=1
set clipboard=unnamed
set showcmd
set ruler
set encoding=utf-8
set showmatch
set relativenumber
set autoindent
set expandtab
set nowrap
set noshowmode
syntax enable


" Themes
colorscheme gruvbox
let g:gruvbox_contrast_dark = "hard"
let t_Co=256
